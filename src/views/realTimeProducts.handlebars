<div>
  <h1>Listado de productos en tiempo real</h1>
  <div id="product-list">
    {{#each products}}
    <div>
      <h3>{{this.title}}</h3>
      <p>{{this.description}}</p>
      <p>Precio: ${{this.price}}</p>
    </div>
    {{/each}}
  </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="js/realTimeProducts.js"></script>